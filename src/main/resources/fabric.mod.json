{
	"schemaVersion": 1,
	"id": "cuisine",
	"version": "${version}",
	"name": "Cuisine",
	"description": "A mod to expand the culinary range in Minecraft.",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/SunkenPotato/cuisine"
	},
	"license": "CC0-1.0",
	"icon": "assets/cuisine/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "com.sunkenpotato.cuisine.Cuisine"
		],
		"client": [
			"com.sunkenpotato.cuisine.HerbalClient"
		],
		"fabric-datagen": [
			"com.sunkenpotato.cuisine.CuisineDataGenerator"
		]
	},
	"mixins": [
		"cuisine.mixins.json",
		{
			"config": "herbal.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
